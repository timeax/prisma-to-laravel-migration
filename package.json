{
  "name": "prisma-laravel-migrate",
  "version": "3.1.25",
  "description": "Generate laravel migrations and/or models using prisma files",
  "bin": {
    "prisma-laravel-migrations": "./dist/cli/migrator.index.js",
    "prisma-laravel-models": "./dist/cli/models.index.js",
    "prisma-laravel-types": "./dist/cli/ts.index.js",
    "prisma-laravel-cli": "./dist/cli/cli.js"
  },
  "keywords": [
    "prisma",
    "laravel",
    "migrations",
    "models",
    "prisma-laravel",
    "prisma-migrations"
  ],
  "homepage": "https://github.com/timeax/prisma-to-laravel-migration#readme",
  "bugs": {
    "url": "https://github.com/timeax/prisma-to-laravel-migration/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timeax/prisma-to-laravel-migration.git"
  },
  "license": "MIT",
  "author": "David Okpako",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npx prisma generate --schema=small.prisma",
    "cjs": "node scripts/toCommons.cjs",
    "gen:laravel": "node scripts/gen-laravel.js",
    "build": "rm -rf dist @types && tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "commit-if-needed": "git diff --quiet || git add . && git commit -m \"Release commit before publish\"",
    "prepublishOnly": "npm run build && npm run commit-if-needed",
    "postpublish": "npm version patch",
    "install:dev": "npm run build && npm link",
    "uninstall:dev": "npm unlink",
    "start": "npm run dev"
  },
  "dependencies": {
    "@prettier/plugin-php": "^0.24.0",
    "@prisma/generator-helper": "^6.19.0",
    "@prisma/internals": "^7.1.0",
    "change-case": "^5.4.4",
    "commander": "^14.0.0",
    "dayjs": "^1.11.13",
    "ejs": "^3.1.10",
    "minimatch": "^10.0.3",
    "node-diff3": "^3.1.2",
    "pluralize": "^8.0.0",
    "prettier": "^3.6.2"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/node": "^24.0.3",
    "jest": "^30.0.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "tsx": "^4.20.3",
    "typescript": "^5.9.3"
  }
}
